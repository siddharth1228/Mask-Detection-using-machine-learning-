    "from flask import Flask, render_template\n",
    "app = Flask(__name__)\n",
    "\n",
    "@app.route('/')\n",
    "def index():\n",
    "  return render_template('index.html')\n",
    "\n",
    "@app.route('/my-link/')\n",
    "def my_link():\n",
    "  print ('I got clicked!')\n",
    "\n",
    "  return 'Click.'\n",
    "\n",
    "if __name__ == '__main__':\n",
    "  app.run(debug=True)"
   ]
  }
